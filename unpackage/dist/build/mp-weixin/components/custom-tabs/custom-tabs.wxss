.custom-tabs{display:flex;position:relative;padding:0 30rpx}.custom-tabs-bar{height:80rpx;line-height:80rpx;color:#979797;padding-right:30rpx;position:relative}.custom-tabs-bar.active{color:#121826;font-weight:500}.tabbar-text{font-size:30rpx}.custom-tabs-cursor{position:absolute;bottom:3px;left:20px;width:20px;height:2px;border-radius:2px;background-color:#2cb5a5;transition:all .3s ease-out}
